@Article{Dalakova2014397,
  Title                    = {Low-temperature nonlinear effects in the conductivity of lightly doped cuprates La<sub>2-x</sub>Sr<sub>x</sub>CuO<sub>4</sub> in antiferromagnetic state},
  Author                   = {Dalakova, N.V. and Belevtsev, B.I. and Belyaev, E.Y. and Panfilov, A.S. and Bobrysheva, N.P. and Selyutin, A.A.},
